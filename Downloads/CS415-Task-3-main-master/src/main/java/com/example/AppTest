package com.example;

import org.junit.jupiter.api.Test;
import static org.junit.jupiter.api.Assertions.assertEquals;

class AppTest {

    @Test
    void testAddition() {
        assertEquals(5, App.add(2, 3));
    }
    @Test
    void testMultiply() {
        assertEquals(5, App.multiply(2, 4));
    }

    @Test
    void testNegativeNumbers() {
        assertEquals(-1, App.add(2, -3));
    }
}
